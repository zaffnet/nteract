{
  "name": "@nteract/notebook-preview",
  "version": "1.0.8",
  "description": "View a notebook using a React Component",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:lib": "babel -d lib src --ignore '**/__tests__/**'",
    "build:lib:watch": "npm run build:lib -- --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nteract/commutable": "^1.1.1",
    "@nteract/display-area": "^1.1.0",
    "@nteract/editor": "^1.0.7",
    "@nteract/transforms": "^1.0.8",
    "codemirror": "~5.24.0",
    "commonmark": "^0.27.0",
    "commonmark-react-renderer": "^4.3.1",
    "mathjax-electron": "^1.2.0"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "react": "^15.4.2"
  },
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause"
}
