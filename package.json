{
  "name": "nteract-monorepo",
  "version": "0.0.0",
  "description": "Interactive literate coding notebook!",
  "repository": {
    "type": "git",
    "url": "https://github.com/nteract/nteract.git"
  },
  "keywords": [
    "jupyter",
    "electron",
    "notebook",
    "nteract",
    "data"
  ],
  "author": "nteract contributors",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/nteract/issues"
  },
  "homepage": "https://github.com/nteract/nteract",
  "scripts": {
    "app:desktop": "lerna run start --scope nteract --stream",
    "app:play": "lerna run dev --scope @nteract/play --stream",
    "app:commuter": "lerna run dev --scope @nteract/commuter --stream",
    "app:jupyter-extension": "lerna run dev --scope nteract-on-jupyter --stream",
    "start": "npm run app:desktop",
    "spawn": "lerna run spawn --scope nteract",
    "spawn:debug": "lerna run spawn:debug --scope nteract",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "NODE_ENV=production npm run build:packages",
    "build:apps": "lerna run build --scope nteract --scope @nteract/play --scope @nteract/commuter --scope nteract-on-jupyter --parallel --stream",
    "build:desktop": "lerna run build --scope nteract --stream",
    "build:desktop:watch": "lerna run build:watch --scope nteract --stream",
    "build:clean:ts": "tsc -b --clean packages",
    "build:packages:ts": "tsc -b packages",
    "build:watch:ts": "tsc -b -w packages",
    "build:packages": "npm run build:packages:ts && NODE_ENV=production lerna run build --parallel --ignore @nteract/play --ignore @nteract/commuter --ignore nteract --ignore nteract-on-jupyter",
    "build:packages:watch": "lerna run build:lib:watch --parallel --ignore @nteract/play --ignore @nteract/commuter --ignore nteract --ignore nteract-on-jupyter",
    "build:icon": "./scripts/make_icons.sh && cd static/icons && iconutil -c icns nteract.iconset && mv nteract.icns ../icon.icns",
    "build:only": "lerna run build --scope",
    "build:watch": "lerna run build:watch --parallel --ignore @nteract/play --ignore nteract",
    "build:watch:only": "lerna run build:watch --stream --scope",
    "check:ship": "yarn && yarn verifyBeforePublish && yarn dist:all && open applications/desktop/dist/mac",
    "ship:desktop": "lerna run ship --scope nteract --stream",
    "docs": "styleguidist server",
    "docs:build": "styleguidist build",
    "predocs:deploy": "npm run docs:build",
    "docs:deploy": "now ./styleguide",
    "docs:promote": "cd ./styleguide && now alias",
    "test": "jest",
    "test:unit": "npm run test",
    "test:coverage": "npm run test -- --coverage",
    "test:lint": "npm run lint",
    "test:flow": "npm run flow",
    "flow": "flow",
    "diagnostics": "scripts/kernelspecs-diagnostics.js",
    "verifyBeforePublish": "tsc -b packages && lerna run prepublishOnly && lerna run prepare",
    "prepublishOnly": "npm run verifyBeforePublish",
    "publish": "lerna run publish",
    "prettify": "prettier --write '**/*.{ts,js,json}' '!**/{dist,lib.git,.next,package.json,build,flow-typed,mathjax-electron}/**'",
    "pack": "lerna run pack --scope nteract --stream",
    "dist": "lerna run dist --scope nteract --stream",
    "dist:all": "lerna run dist:all --scope nteract --stream",
    "clean": "lerna clean --yes && rimraf node_modules"
  },
  "jest": {
    "testURL": "http://localhost",
    "setupFiles": [
      "raf/polyfill",
      "./scripts/test-shim",
      "./scripts/test-setup"
    ],
    "moduleNameMapper": {
      "^@nteract/styles$": "<rootDir>/packages/styles/lib",
      "^@nteract/core/dummy$": "<rootDir>/packages/core/src/dummy",
      "^@nteract/vega-embed-v2": "@nteract/vega-embed-v2",
      "^@nteract/([^/]*)$": "<rootDir>/packages/$1/src",
      "^(ansi-to-react|enchannel-zmq-backend|fs-observable|rx-binder|rx-jupyter)$": "<rootDir>/packages/$1/src",
      "\\.css$": "<rootDir>/scripts/noop-module.js"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/scripts",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "transform": {
      ".*": "babel-jest"
    }
  },
  "lint-staged": {
    "*.ts,*.js,!*js.snap": [
      "prettier --write",
      "git add"
    ],
    "*.json,!package.json": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/plugin-transform-typescript": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babel/runtime-corejs2": "^7.0.0",
    "@types/immutable": "^3.8.7",
    "@types/react": "^16.4.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-lodash": "^3.3.2",
    "concurrently": "^4.0.0",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "3.0.6",
    "electron-builder": "20.29.0",
    "electron-context-menu": "^0.10.0",
    "electron-log": "^2.2.13",
    "electron-updater": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-perf": "^2.0.9",
    "flow-bin": "^0.81.0",
    "flow-copy-source": "^2.0.0",
    "husky": "^1.0.0",
    "immutable": "^4.0.0-rc.9",
    "jest": "^23.0.1",
    "jupyter-paths": "^2.0.0",
    "kernelspecs": "^2.0.0",
    "lerna": "^3.0.0",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "mkdirp": "^0.5.1",
    "node-fetch": "2.1.2",
    "prettier": "^1.11.0",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-notification-system": "^0.2.16",
    "react-redux": "^5.0.7",
    "react-styleguidist": "^8.0.0",
    "react-test-renderer": "^16.2.0",
    "redux": "^4.0.0",
    "redux-electron-store": "^0.6.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "regenerator-runtime": "^0.12.1",
    "remark-parse": "^5.0.0",
    "remark-stringify": "^5.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "shell-env": "^2.0.0",
    "spawn-rx": "^3.0.0",
    "spawnteract": "^5.0.0",
    "style-loader": "^0.23.0",
    "styled-jsx": "^3.1.0",
    "typescript": "^3.1.3",
    "unified": "^7.0.0",
    "unist-builder": "^1.0.2",
    "webfontloader": "^1.6.28",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4",
    "yargs": "^12.0.0"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "applications/*",
      "applications/jupyter-extension/nteract_on_jupyter"
    ],
    "nohoist": [
      "**/nteract/**"
    ]
  },
  "resolutions": {
    "node-fetch": "2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
