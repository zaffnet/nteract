{
  "name": "nteract",
  "version": "0.5.2",
  "description": "Interactive literate coding notebook!",
  "main": "./lib/webpacked-main.js",
  "repository": "nteract/nteract",
  "keywords": [
    "jupyter",
    "electron",
    "notebook",
    "nteract",
    "data"
  ],
  "author": "nteract contributors",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/nteract/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://nteract.io",
  "scripts": {
    "clean": "rimraf lib dist",
    "postinstall": "electron-builder install-app-deps",
    "prestart": "npm run build",
    "start": "npm run spawn",
    "spawn": "cross-env NODE_ENV=development electron .",
    "spawn:debug": "cross-env DEBUG=true NODE_ENV=development electron .",
    "prebuild": "rimraf lib",
    "build": "webpack --config webpack.dev.js --progress --colors",
    "build:watch": "npm run build -- --watch",
    "pack": "npm run clean && webpack --config webpack.prod.js && electron-builder --dir",
    "dist": "npm run clean && webpack --config webpack.prod.js && electron-builder",
    "publish": "npm run clean && webpack --config webpack.prod.js && electron-builder -p always",
    "dist:all": "npm run dist -- -mlw",
    "publish:all": "npm run publish -- -mlw"
  },
  "build": {
    "appId": "io.nteract.nteract",
    "productName": "nteract",
    "publish": [
      {
        "provider": "github"
      }
    ],
    "fileAssociations": {
      "ext": "ipynb",
      "name": "ipynb"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "nsis": {
      "perMachine": true,
      "oneClick": false
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "maintainer": "nteract contributors <jupyter@googlegroups.com>",
      "target": [
        "deb",
        "AppImage",
        "tar.gz"
      ],
      "desktop": {
        "Comment": "Interactive literate coding notebook",
        "Exec": "/opt/nteract/nteract %U",
        "Icon": "nteract",
        "MimeType": "application/x-ipynb+json",
        "Name": "nteract",
        "Path": "/opt/nteract/",
        "StartupNotify": "true",
        "Terminal": "false",
        "Type": "Application",
        "Categories": "Science;Development;"
      },
      "category": "Science",
      "packageCategory": "editors"
    },
    "files": [
      "lib/*.js",
      "lib/*.css",
      "lib/*.woff",
      "lib/*.woff2",
      "static"
    ],
    "extraResources": [
      "bin",
      "example-notebooks"
    ],
    "npmSkipBuildFromSource": true
  },
  "jest": {
    "setupFiles": [
      "./scripts/mockument"
    ]
  },
  "dependencies": {
    "github": "^9.0.0",
    "ijavascript": "^5.0.17",
    "jmp": "^1.0.0",
    "mathjax-electron": "^2.0.1",
    "redux-electron-store": "^0.4.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@nteract/commutable": "^3.0.1",
    "@nteract/core": "^1.0.2",
    "@nteract/display-area": "^3.1.3",
    "@nteract/editor": "^5.0.2",
    "@nteract/messaging": "^3.0.1",
    "@nteract/transforms-full": "^4.0.3",
    "@nteract/types": "^1.0.1",
    "commonmark": "0.28.1",
    "commonmark-react-renderer": "4.3.4",
    "cross-env": "5.1.3",
    "date-fns": "1.29.0",
    "electron": "1.7.10",
    "electron-builder": "19.49.4",
    "electron-context-menu": "0.9.1",
    "electron-log": "2.2.13",
    "electron-updater": "2.17.8",
    "enchannel-zmq-backend": "^6.0.1",
    "escape-carriage": "1.2.0",
    "fs-observable": "^2.0.3",
    "jupyter-paths": "2.0.0",
    "kernelspecs": "2.0.0",
    "lodash": "4.17.4",
    "nteract-assets": "4.0.0",
    "react-dnd": "2.5.4",
    "react-dnd-html5-backend": "2.5.4",
    "react-notification-system": "0.2.16",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-observable": "0.17.0",
    "rimraf": "2.6.2",
    "rxjs": "5.5.6",
    "shell-env": "1.0.0",
    "spawn-rx": "2.0.12",
    "spawnteract": "4.0.0",
    "styled-jsx": "2.2.1",
    "yargs": "10.0.3"
  }
}
